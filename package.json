{
  "name": "server-names-api",
  "version": "1.0.0",
  "description": "Server para aulas de operadores rxjs",
  "main": "index.js",
  "repository": "https://github.com/alexclemente1985/AngularTrainings.git",
  "author": "Alexandre Pinheiro <alexclemente1985@gmail.com>",
  "license": "MIT",
  "scripts": {
    "env:config": "ts-node ./src/assets/scripts/setEnv.ts",
    "generate:dev": "yarn env:config -- --environment=dev && ts-node ./src/config/generate.ts",
    "generate": "yarn env:config -- --environment=prod && ts-node ./src/config/generate.ts",
    "start": "yarn env:config -- --environment=prod && yarn tsc && node dist/server.js",
    "start:dev": "yarn env:config -- --environment=dev && yarn tsc && node dist/server.js",
    "dev": "yarn env:config -- --environment=dev && ts-node-dev --inspect --transpile-only --ignore-watch node_modules --respawn src/server.ts"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-beta.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "uuid": "^8.3.2",
    "yargs": "^17.3.1",
    "mongoose": "^6.2.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.21",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  }
}
